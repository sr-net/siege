{
  "name": "@sr-net/siege",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "type": "module",
  "private": true,
  "repository": {
    "type": "github",
    "url": "sr-net/siege"
  },
  "packageManager": "pnpm@9.15.6",
  "engines": {
    "node": "^22"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,yml}": [
      "prettier --write"
    ]
  },
  "imports": {
    "#r/*": "./src/*",
    "#dbschema/*.ts": "./dbschema/*.ts"
  },
  "scripts": {
    "dev": "node --experimental-strip-types --watch --env-file .env src/index.ts",
    "db:reset": "nu scripts/db-reset.nu",
    "docker:build": "docker build . --tag siege",
    "docker:prune": "docker image prune --force",
    "docker:run": "docker run --rm --name siege -p 3000:3000 siege",
    "gen": "npx @edgedb/generate interfaces",
    "lint": "eslint 'src/**/*.ts'",
    "postinstall": "simple-git-hooks",
    "snapshot": "node --experimental-strip-types src/index.ts -shot",
    "start": "node --experimental-strip-types src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit -P tsconfig.json"
  },
  "resolutions": {
    "esbuild": "0.24.2"
  },
  "dependencies": {
    "@gqloom/valibot": "0.7.1",
    "@hono/graphql-server": "0.5.1",
    "@hono/node-server": "1.13.8",
    "edgedb": "1.5.12",
    "graphql": "16.10.0",
    "hono": "4.7.2",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "ts-dedent": "2.2.0",
    "valibot": "1.0.0-rc.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.16.0",
    "@edgedb/generate": "0.6.1",
    "@tsconfig/node22": "22.0.0",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.13.5",
    "eslint": "9.21.0",
    "lint-staged": "15.4.3",
    "prettier": "3.5.2",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.7.3",
    "vite": "6.2.0",
    "vitest": "3.0.7"
  },
  "pnpm": {
    "overrides": {
      "@eslint/markdown": "-",
      "@stylistic/eslint-plugin": "-",
      "eslint-plugin-jsonc": "-",
      "eslint-plugin-toml": "-",
      "eslint-plugin-vue": "-",
      "eslint-processor-vue-blocks": "-",
      "eslint-plugin-yml": "-",
      "jsonc-eslint-parser": "-",
      "toml-eslint-parser": "-",
      "yaml-eslint-parser": "-",
      "is-core-module": "npm:@nolyfill/is-core-module@^1"
    }
  }
}
