{
  "name": "@sr-net/siege",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "private": true,
  "repository": {
    "type": "github",
    "url": "sr-net/siege"
  },
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "node": "^18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,yml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "",
    "lint": "eslint 'src/**/*.ts'",
    "migrate": "typeorm migration:run",
    "migrate:create": "typeorm migration:create -d migrations -n",
    "migrate:generate": "typeorm migration:generate -d migrations -n",
    "postinstall": "simple-git-hooks",
    "snapshot": "ts-node -T src/index.ts -shot",
    "start": "ts-node -T --files src/index.ts",
    "test": "NODE_ENV=test jest -i",
    "test:coverage": "pnpm run test --coverage",
    "typecheck": "tsc --noEmit -P tsconfig.json",
    "typeorm": "ts-node -T ./node_modules/typeorm/cli.js"
  },
  "resolutions": {
    "@types/node": "18.7.16"
  },
  "dependencies": {
    "@abraham/reflection": "0.10.0",
    "@sentry/node": "6.13.2",
    "apollo-server-core": "3.10.2",
    "apollo-server-express": "3.10.2",
    "class-validator": "0.13.2",
    "cookie": "0.5.0",
    "cors": "2.8.5",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "graphql": "15.6.0",
    "helmet": "4.6.0",
    "pg": "8.8.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "type-graphql": "1.2.0-rc.1",
    "typeorm": "0.2.45",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.8.0",
    "@tsconfig/node18": "1.0.1",
    "@types/cookie": "0.5.1",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "29.0.1",
    "@types/node": "18.7.16",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "eslint": "8.23.0",
    "jest": "29.0.3",
    "lint-staged": "11.2.3",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.6.1",
    "supertest": "6.1.6",
    "ts-jest": "29.0.0",
    "typescript": "4.8.3"
  }
}
