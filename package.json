{
  "name": "@sr-net/siege",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "private": true,
  "repository": {
    "type": "github",
    "url": "sr-net/siege"
  },
  "packageManager": "pnpm@8.11.0",
  "engines": {
    "node": "^20"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,yml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "na nodemon --exec tsx src/index.ts",
    "db:reset": "nu scripts/db-reset.nu",
    "docker:build": "docker build . --tag siege",
    "docker:prune": "docker image prune --force",
    "docker:run": "docker run --rm --name siege -p 3000:3000 siege",
    "gen": "npx @edgedb/generate interfaces",
    "lint": "eslint 'src/**/*.ts'",
    "postinstall": "simple-git-hooks",
    "snapshot": "tsx src/index.ts -shot",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit -P tsconfig.json"
  },
  "resolutions": {
    "@types/node": "20.10.0"
  },
  "dependencies": {
    "@fastify/cookie": "9.2.0",
    "@fastify/cors": "8.4.1",
    "@fastify/helmet": "11.1.1",
    "@tsconfig/node20": "20.1.2",
    "dotenv": "16.3.1",
    "edgedb": "1.4.1",
    "fastify": "4.24.3",
    "graphql": "16.8.1",
    "mercurius": "13.3.1",
    "nexus": "1.3.0",
    "pino": "8.16.2",
    "pino-pretty": "10.2.3",
    "ts-dedent": "2.2.0",
    "tsconfig-paths": "4.2.0",
    "tsx": "3.14.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "4.0.0",
    "@edgedb/generate": "0.4.1",
    "@types/node": "20.10.0",
    "@types/uuid": "9.0.7",
    "eslint": "8.54.0",
    "lint-staged": "15.1.0",
    "nodemon": "3.0.1",
    "prettier": "3.1.0",
    "simple-git-hooks": "2.9.0",
    "typescript": "5.3.2",
    "vitest": "0.34.6"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "array.prototype.tosorted": "npm:@nolyfill/array.prototype.tosorted@latest",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@latest",
      "has": "npm:@nolyfill/has@latest",
      "object.assign": "npm:@nolyfill/object.assign@latest",
      "object.entries": "npm:@nolyfill/object.entries@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.hasown": "npm:@nolyfill/object.hasown@latest",
      "object.values": "npm:@nolyfill/object.values@latest",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@latest"
    }
  }
}
