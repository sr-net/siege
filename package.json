{
  "name": "@sr-net/siege",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "private": true,
  "repository": {
    "type": "github",
    "url": "sr-net/siege"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": "^16"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,yml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --no-deps -r tsconfig-paths/register --files --inspect -- src/index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "migrate": "yarn typeorm migration:run",
    "migrate:create": "yarn typeorm migration:create -d migrations -n",
    "migrate:generate": "yarn typeorm migration:generate -d migrations -n",
    "postinstall": "simple-git-hooks",
    "snapshot": "ts-node -T src/index.ts -shot",
    "start": "ts-node -T --files src/index.ts",
    "test": "NODE_ENV=test jest -i",
    "test:coverage": "yarn run test --coverage",
    "typecheck": "yarn tsc --noEmit -P tsconfig.json",
    "typeorm": "ts-node -T ./node_modules/typeorm/cli.js"
  },
  "resolutions": {
    "@types/node": "16.11.45"
  },
  "dependencies": {
    "@sentry/node": "7.7.0",
    "apollo-server-express": "3.3.0",
    "class-validator": "0.13.2",
    "cookie": "0.4.1",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "graphql": "15.8.0",
    "helmet": "4.6.0",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "ts-node": "10.9.1",
    "tsconfig-paths": "3.14.1",
    "type-graphql": "1.1.1",
    "typeorm": "0.2.45",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.3.0",
    "@types/cookie": "0.4.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.45",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "eslint": "7.32.0",
    "jest": "27.5.1",
    "jest-junit": "13.2.0",
    "lint-staged": "13.0.3",
    "prettier": "2.4.1",
    "simple-git-hooks": "2.8.0",
    "supertest": "6.2.4",
    "ts-jest": "27.0.5",
    "ts-node-dev": "2.0.0",
    "typescript": "4.4.3"
  }
}
