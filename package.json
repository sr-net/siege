{
  "name": "@sr-net/siege",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "private": true,
  "repository": {
    "type": "github",
    "url": "sr-net/siege"
  },
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "node": "^18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,yml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "",
    "docker:build": "docker build . --tag siege",
    "docker:prune": "docker image prune --force",
    "docker:run": "docker run --rm --name siege -p 3000:3000 siege",
    "lint": "eslint 'src/**/*.ts'",
    "postinstall": "simple-git-hooks",
    "snapshot": "ts-node -T src/index.ts -shot",
    "start": "ts-node -T --files src/index.ts",
    "test": "NODE_ENV=test jest",
    "typecheck": "tsc --noEmit -P tsconfig.json",
    "typeorm": "ts-node -T ./node_modules/typeorm/cli.js"
  },
  "resolutions": {
    "@types/node": "18.7.17"
  },
  "dependencies": {
    "@abraham/reflection": "0.10.0",
    "@fastify/cookie": "8.1.0",
    "@fastify/cors": "8.1.0",
    "@fastify/helmet": "10.0.0",
    "@sentry/node": "7.12.1",
    "@tsconfig/node18": "1.0.1",
    "class-validator": "0.13.2",
    "dotenv": "16.0.2",
    "fastify": "4.5.3",
    "graphql": "15.6.0",
    "mercurius": "11.0.0",
    "pg": "8.8.0",
    "pino": "8.5.0",
    "pino-pretty": "9.1.0",
    "remeda": "1.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "type-graphql": "1.2.0-rc.1",
    "typeorm": "0.3.9",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.8.0",
    "@types/jest": "29.0.2",
    "@types/node": "18.7.17",
    "@types/uuid": "8.3.4",
    "eslint": "8.23.1",
    "jest": "29.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.8.0",
    "ts-jest": "29.0.1",
    "typescript": "4.8.3"
  }
}
