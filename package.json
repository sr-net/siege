{
  "name": "@sr-net/siege",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "private": true,
  "repository": {
    "type": "github",
    "url": "sr-net/siege"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": "^20"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,yml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "na nodemon --exec tsx src/index.ts",
    "db:reset": "nu scripts/db-reset.nu",
    "docker:build": "docker build . --tag siege",
    "docker:prune": "docker image prune --force",
    "docker:run": "docker run --rm --name siege -p 3000:3000 siege",
    "gen": "npx @edgedb/generate interfaces",
    "lint": "eslint 'src/**/*.ts'",
    "postinstall": "simple-git-hooks",
    "snapshot": "tsx src/index.ts -shot",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit -P tsconfig.json"
  },
  "resolutions": {
    "@types/node": "20.8.4"
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/cors": "8.4.0",
    "@fastify/helmet": "10.1.1",
    "@sentry/node": "7.73.0",
    "@tsconfig/node20": "20.1.2",
    "class-validator": "0.14.0",
    "dotenv": "16.3.1",
    "edgedb": "1.3.6",
    "fastify": "4.23.2",
    "graphql": "16.8.1",
    "mercurius": "12.2.0",
    "nexus": "1.3.0",
    "pino": "8.16.0",
    "pino-pretty": "10.2.3",
    "remeda": "1.27.1",
    "ts-dedent": "2.2.0",
    "tsconfig-paths": "4.2.0",
    "tsx": "3.13.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "4.0.0",
    "@edgedb/generate": "0.3.3",
    "@types/node": "20.8.4",
    "@types/uuid": "9.0.5",
    "eslint": "8.51.0",
    "lint-staged": "14.0.1",
    "nodemon": "3.0.1",
    "p-queue": "7.4.1",
    "prettier": "3.0.3",
    "simple-git-hooks": "2.9.0",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vitest": "0.34.6"
  }
}
