{
  "name": "@sr-net/siege",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "private": true,
  "repository": {
    "type": "github",
    "url": "sr-net/siege"
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": "^20"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,yml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "na nodemon --exec tsx src/index.ts",
    "db:reset": "nu scripts/db-reset.nu",
    "docker:build": "docker build . --tag siege",
    "docker:prune": "docker image prune --force",
    "docker:run": "docker run --rm --name siege -p 3000:3000 siege",
    "gen": "npx @edgedb/generate interfaces",
    "lint": "eslint 'src/**/*.ts'",
    "postinstall": "simple-git-hooks",
    "snapshot": "tsx src/index.ts -shot",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit -P tsconfig.json"
  },
  "dependencies": {
    "@hono/graphql-server": "0.4.3",
    "@hono/node-server": "1.11.4",
    "@tsconfig/node20": "20.1.4",
    "dotenv": "16.4.5",
    "edgedb": "1.5.7",
    "graphql": "16.9.0",
    "hono": "4.4.7",
    "nexus": "1.3.0",
    "pino": "9.2.0",
    "pino-pretty": "11.2.1",
    "ts-dedent": "2.2.0",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.15.7",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.21.1",
    "@edgedb/generate": "0.5.3",
    "@types/node": "20.14.8",
    "@types/uuid": "10.0.0",
    "eslint": "9.17.0",
    "lint-staged": "15.2.7",
    "nodemon": "3.1.4",
    "prettier": "3.3.2",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.5.2",
    "vitest": "1.6.0"
  },
  "pnpm": {
    "overrides": {
      "hasown": "npm:@nolyfill/hasown@latest"
    }
  }
}
