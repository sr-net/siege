{
  "name": "@sr-net/siege",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "private": true,
  "repository": {
    "type": "github",
    "url": "sr-net/siege"
  },
  "packageManager": "pnpm@7.26.3",
  "engines": {
    "node": "^18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,yml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "na nodemon --exec tsx src/index.ts",
    "db:reset": "nu scripts/db-reset.nu",
    "docker:build": "docker build . --tag siege",
    "docker:prune": "docker image prune --force",
    "docker:run": "docker run --rm --name siege -p 3000:3000 siege",
    "gen": "npx @edgedb/generate interfaces",
    "lint": "eslint 'src/**/*.ts'",
    "postinstall": "simple-git-hooks",
    "snapshot": "tsx src/index.ts -shot",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit -P tsconfig.json"
  },
  "resolutions": {
    "@types/node": "18.11.19"
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/cors": "8.2.0",
    "@fastify/helmet": "10.1.0",
    "@sentry/node": "7.36.0",
    "@tsconfig/node18": "1.0.1",
    "class-validator": "0.14.0",
    "dotenv": "16.0.3",
    "edgedb": "1.0.2",
    "fastify": "4.12.0",
    "graphql": "16.6.0",
    "mercurius": "12.0.0",
    "nexus": "1.3.0",
    "pino": "8.8.0",
    "pino-pretty": "9.1.1",
    "remeda": "1.6.1",
    "ts-dedent": "2.2.0",
    "tsconfig-paths": "4.1.2",
    "tsx": "3.12.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "1.1.2",
    "@edgedb/generate": "0.0.7",
    "@types/node": "18.11.19",
    "@types/uuid": "9.0.0",
    "eslint": "8.33.0",
    "lint-staged": "13.1.0",
    "nodemon": "2.0.20",
    "p-queue": "7.3.3",
    "prettier": "2.8.3",
    "simple-git-hooks": "2.8.1",
    "typescript": "4.9.5",
    "vite": "3.2.5",
    "vitest": "0.28.4"
  }
}
