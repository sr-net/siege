{
  "name": "@sr-net/siege",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "private": true,
  "repository": {
    "type": "github",
    "url": "sr-net/siege"
  },
  "packageManager": "pnpm@7.12.1",
  "engines": {
    "node": "^18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,yml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "na nodemon --exec tsx src/index.ts",
    "db:reset": "nu scripts/db-reset.nu",
    "docker:build": "docker build . --tag siege",
    "docker:prune": "docker image prune --force",
    "docker:run": "docker run --rm --name siege -p 3000:3000 siege",
    "gen": "edgeql-js --output-dir src/edgedb",
    "lint": "eslint 'src/**/*.ts'",
    "postinstall": "simple-git-hooks",
    "snapshot": "tsx src/index.ts -shot",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit -P tsconfig.json"
  },
  "resolutions": {
    "@types/node": "18.7.18"
  },
  "dependencies": {
    "@fastify/cookie": "8.2.0",
    "@fastify/cors": "8.1.0",
    "@fastify/helmet": "10.0.0",
    "@sentry/node": "7.13.0",
    "@tsconfig/node18": "1.0.1",
    "class-validator": "0.13.2",
    "dotenv": "16.0.2",
    "edgedb": "0.22.7",
    "fastify": "4.8.1",
    "graphql": "16.6.0",
    "mercurius": "11.0.0",
    "nexus": "1.3.0",
    "pino": "8.6.0",
    "pino-pretty": "9.1.0",
    "remeda": "1.1.0",
    "ts-dedent": "2.2.0",
    "tsconfig-paths": "4.1.0",
    "tsx": "3.9.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.8.0",
    "@types/node": "18.7.18",
    "@types/uuid": "8.3.4",
    "eslint": "8.25.0",
    "lint-staged": "13.0.3",
    "nodemon": "2.0.20",
    "p-queue": "7.3.0",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.8.0",
    "typescript": "4.8.3",
    "vitest": "0.24.0"
  }
}
